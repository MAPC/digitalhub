<section class="banner">
  <div class="container">
    <div class="content-column banner-content">
      <h1>Share Your Ideas</h1>
    </div>
    <%= image_tag("mapc_logo.svg", class: "logo", alt: t('logo_alt')) %>
  </div>
</section>
<section class="content">
  <div class="container">
    <div class="content-column">
      <h1>Editing Story</h1>

      <%= render 'form', story: @story %>
    </div>
  </div>
  <%= link_to 'Back', story_path(@story), class: 'link-button-row' %>
</section>
