<%= render "banner" %>
<section class="content">
  <div class="container">
    <div class="content-column">
      <h1>Stories</h1>
        <% @stories.each do |story| %>
          <%= render partial: 'media', locals: { media: story } %>
        <% end %>
      </div>
    </div>
  <%= link_to 'Speak Out!', new_story_path, class: 'link-button-row' %>
</section>
